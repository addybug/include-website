<!DOCTYPE html>
<html lang="en">

    <%- include('../partials/head.ejs'); -%>

    <body>

        <%- include('../partials/nav'); -%>

        <section class="breadcrumb_area br_image">
          <div class="container">
            <div class="page-cover text-center">
              <h2 class="page-cover-tittle"><%= info.day %></h2>
            </div>
          </div>
        </section>
        <section class="event_details_area section_gap">
          <div class="container">
            <div class="row">
              <div class="col-md-12 event_details">
                <h2 class="res_eve_des assignment-header" >
                  Assignments for Today
                </h2>
                <h2
                  class="res_eve_des project-header"
                >
                  <%= info.title %>
                </h2>
                <div class='paragraph-container'>
                  <%- info.description %>
                </div>
                <h2 class="res_eve_des assignment-header <%= info.viewAttachments %>">
                  Attachments
                </h2>
                <div class="starter-code <%= info.viewAttachments %>">
                <a
                  href="<%= info.starter1 %>"
                  class="res_eve_des starter-color"
                  >Starter Code 1 <br />
                </a>
              </div>
              <div class="starter-code <%= info.view2 %>">
                <a
                  href="<%= info.starter2 %>"
                  class="res_eve_des starter-color"
                  >Starter Code 2
                </a>
                </div>
              </div>
            </div>
          </div>
        </section>

        <%- include('../partials/footer.ejs'); -%>

        <%- include('../partials/scripts.ejs'); -%>

    </body>
</html>
